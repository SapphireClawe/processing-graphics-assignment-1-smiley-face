void setup() {
  size(700, 500);
  background(42, 196, 247);


}

void draw() {
  stroke(58, 232, 42);
    strokeWeight(1);
    fill(227, 123, 5);
    rect(70, 50, 70, 40);
  stroke(242, 7, 11);
    strokeWeight(4);
    fill(227, 5, 97);
    rect(210, 50, 70, 40);
  stroke(242, 242, 7);
    strokeWeight(14);
    fill(3, 112, 7);
    rect(350, 50, 70, 40);
  
  
  ellipse(350, 250, 200, 200);

}
